<%- include('partials/header') %>

<% for (let pokemon of pokemons) { %>
  <div class="thumbnail">
    <a href="/pokemon/<%= pokemon.numero %>">
      <img src="/img/<%= pokemon.numero %>.png" alt="Illustration du Pokemon <%= pokemon.nom %>.">
      <p><%= pokemon.nom %></p>
    </a>
  </div>
<% } %>

<%- include('partials/footer') %>